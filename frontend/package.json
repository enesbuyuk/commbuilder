{
  "name": "commbuilder",
  "version": "0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -f ../.env -- next dev --turbopack",
    "dev-prebuild": "dotenvx run -f ../.env -- node ./scripts/fetchTranslations.js",
    "dev-build": "dotenvx run -f ../.env -- next build",
    "dev-start": "dotenvx run -f ../.env -- next start",
    "prebuild": "node ./scripts/fetchTranslations.js",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "standalone": "next build && cp -r public/ .next/standalone && cp -r .next/static .next/standalone/.next"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "next-intl": "^4.3.12",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-simple-typewriter": "^5.0.1",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "eslint": "^9.37.0",
    "eslint-config-next": "15.5.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.18.2",
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.2",
      "@types/react-dom": "19.2.1"
    }
  }
}
